{
  "name": "datapage",
  "version": "1.3.4",
  "description": "Simple Pagination Data Object",
  "main": "datapage.js",
  "types": "dist/datapage.d.ts",
  "type": "module",
  "scripts": {
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "build": "rollup -c",
    "build:vite": "vite build",
    "build:types": "tsc --emitDeclarationOnly",
    "dev": "npm run lint && npm run test && npm run build",
    "watch": "rollup -c --watch",
    "lint": "eslint src/ spec/",
    "lint:fix": "eslint src/ spec/ --fix",
    "format": "prettier --write src/ spec/",
    "format:check": "prettier --check src/ spec/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/trapple/datapagejs.git"
  },
  "author": "trapple",
  "license": "Artistic License",
  "bugs": {
    "url": "https://github.com/trapple/datapagejs/issues"
  },
  "homepage": "https://github.com/trapple/datapagejs",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@rollup/plugin-terser": "^0.4.4",
    "@types/node": "^24.0.12",
    "eslint": "^9.30.1",
    "jsdom": "^26.1.0",
    "prettier": "^3.6.2",
    "rollup": "^4.44.2",
    "typescript": "^5.8.3",
    "vite": "^7.0.4",
    "vitest": "^3.2.4"
  },
  "keywords": [
    "pagenation",
    "pager",
    "page"
  ]
}
