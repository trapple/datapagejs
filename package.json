{
  "name": "datapage",
  "version": "1.3.4",
  "description": "Simple Pagination Data Object",
  "main": "dist/datapage.esm.js",
  "module": "dist/datapage.esm.js",
  "types": "dist/datapage.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/datapage.esm.js",
      "require": "./index.cjs",
      "default": "./dist/datapage.esm.js"
    }
  },
  "scripts": {
    "test": "vitest run && node spec/commonjs.spec.cjs",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "build": "npm run build:types && rollup -c",
    "build:rollup": "rollup -c",
    "build:types": "tsc --emitDeclarationOnly",
    "dev": "npm run lint && npm run test && npm run build",
    "watch": "rollup -c --watch",
    "lint": "eslint src/ spec/ --ext .js,.ts",
    "lint:fix": "eslint src/ spec/ --ext .js,.ts --fix",
    "format": "prettier --write src/ spec/",
    "format:check": "prettier --check src/ spec/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/trapple/datapagejs.git"
  },
  "author": "trapple",
  "license": "Artistic License",
  "bugs": {
    "url": "https://github.com/trapple/datapagejs/issues"
  },
  "homepage": "https://github.com/trapple/datapagejs",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.4",
    "@types/node": "^24.0.12",
    "@typescript-eslint/eslint-plugin": "^8.36.0",
    "@typescript-eslint/parser": "^8.36.0",
    "eslint": "^9.30.1",
    "jsdom": "^26.1.0",
    "prettier": "^3.6.2",
    "rollup": "^4.44.2",
    "tslib": "^2.8.1",
    "typescript": "^5.8.3",
    "vite": "^7.0.4",
    "vitest": "^3.2.4"
  },
  "keywords": [
    "pagenation",
    "pager",
    "page"
  ]
}
